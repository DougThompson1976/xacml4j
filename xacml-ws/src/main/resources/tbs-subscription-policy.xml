<?xml version="1.0" encoding="UTF-8"?>
<PolicySet
      xmlns="urn:oasis:names:tc:xacml:2.0:policy:schema:os"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="urn:oasis:names:tc:xacml:2.0:policy:schema:os http://docs.oasis-open.org/xacml/2.0/access_control-xacml-2.0-policy-schema-os.xsd"
      PolicySetId="urn:comcast:names:tc:xacml:1.0:policy:tve:channels:subscription:tbs"
      PolicyCombiningAlgId="urn:oasis:names:tc:xacml:1.0:policy-combining-algorithm:deny-overrides">
    <Description>A TBS subscription policy set</Description>
    <Target/>
    <Policy RuleCombiningAlgId="urn:oasis:names:tc:xacml:1.0:rule-combining-algorithm:deny-overrides" 
    	PolicyId="urn:oasis:names:tc:xacml:1.0:policy:tve:channels:subscription">
    	<Target/>
    	<Rule Effect="Permit" RuleId="urn:comcast:names:tc:xacml:1.0:rule:tve:channels:subscription:tbs">
    		<Target/>
    		<Condition>
    			<AttributeValue DataType="http://www.w3.org/2001/XMLSchema#boolean">true</AttributeValue>
    		</Condition>
    	</Rule>
    </Policy> 
</PolicySet>
